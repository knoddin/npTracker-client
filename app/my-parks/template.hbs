<h2>My Parks</h2>


  {{#each model as |park|}}
  {{#if park.editable}}
    <div>
      <h3 id="park-titles" class="my-parks">{{park.name}}, {{park.location}} </h3>
      <h4 id="park-description" class="my-parks">{{park.description}}</h4>

      {{#link-to "update-park" park}}
      <button class="btn btn-default btn-sm">
        Edit Park
      </button>
        {{/link-to}}

      {{#link-to 'delete-park' park}}
      <button id="delete" class="btn btn-default btn-sm">
        Delete
      </button>
      {{/link-to}}

    </div>

    {{/if}}
  {{/each}}
